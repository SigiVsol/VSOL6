<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" styleClass="background" stylesheets="@/fx.css/app.fx.css" xmlns="http://javafx.com/javafx/14.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.vsol.vsol6.controller.fx.app.UserSelection">
   <children>
      <Label fx:id="organization" styleClass="boldTitle" text="Veterinary Solutions" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="10.0" top="20.0" />
         </VBox.margin>
      </Label>
      <Label text="Select an user" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="extraLargeText" />
            <String fx:value="grayText" />
         </styleClass>
      </Label>
      <FlowPane fx:id="nameFP" alignment="TOP_CENTER">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin></FlowPane>
      <Button mnemonicParsing="false" onAction="#back">
         <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/icons/white/back.png" />
               </image>
            </ImageView>
         </graphic>
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
