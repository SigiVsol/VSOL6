<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" styleClass="background" stylesheets="@/fx.css/app.fx.css" xmlns="http://javafx.com/javafx/14.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.vsol.vsol6.controller.fx.Login">
   <children>
      <Label fx:id="lblTitle" alignment="TOP_CENTER" styleClass="boldTitle" text="Title" textAlignment="CENTER" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="lblDescription" alignment="TOP_CENTER" text="Description" textAlignment="CENTER" VBox.vgrow="NEVER">
         <styleClass>
            <String fx:value="extraLargeText" />
            <String fx:value="grayText" />
         </styleClass>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" prefHeight="500.0" prefWidth="400.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="btnBack" mnemonicParsing="false" onAction="#back">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@/icons/white/back.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </VBox>
            <FlowPane fx:id="fpList" alignment="TOP_CENTER" prefWidth="500.0" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </FlowPane>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="btnBack1" mnemonicParsing="false" onAction="#back" visible="false">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@/icons/white/back.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0">
         <children>
            <ImageView fx:id="imgLogo" fitHeight="128.0" fitWidth="128.0" opacity="0.2" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/icons/colored/logo.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </children>
</VBox>
